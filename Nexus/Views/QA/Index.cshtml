
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-3">
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-dark">CAPAS</h6>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="form-group col-12">
                        <label>Subir información</label>
                        <input type="file" class="form-control form-control-sm" id="txtCapasImport_file" name="name" value="" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-3 offset-9">
                        <div class="form-group">
                            <button type="button" class="btn-sm btn btn-icon btn-success btn-left" id="btnCapasImport_save"><i class="fa fa-file-upload"></i> Upload</button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="progress" role="progressbar" aria-label="Info example" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-info text-dark" id="pgbCapasImport_progress"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-dark">Desviaciones</h6>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="form-group col-12">
                        <label>Subir información</label>
                        <input type="file" class="form-control form-control-sm" id="" name="name" value="" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-3 offset-9">
                        <div class="form-group">
                            <button type="button" class="btn-sm btn btn-icon btn-success btn-left" id="btnOrdenesTrabajo_file_save"><i class="fa fa-file-upload"></i> Upload</button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="progress" role="progressbar" aria-label="Info example" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
                            <div class="progress-bar bg-info text-dark" style="width: 0%"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-dark">Vencimiento de PNOs</h6>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="form-group col-12 input-container">
                        <label>Subir información</label>
                        <input type="file" class="form-control form-control-sm" id="txtPnosImport_file" name="name" value="" />
                    </div>
                </div>
                <div class="row">
                    <div class="col-3 offset-9">
                        <div class="form-group">
                            <button type="button" class="btn-sm btn btn-icon btn-success btn-left" id="btnPnosImport_save"><i class="fa fa-file-upload"></i> Upload</button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="progress" role="progressbar" aria-label="Info example" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
                            <div class="progress-bar progress-bar-striped progress-bar-animated bg-info text-dark w-0" id="pgbPnosImport_progress"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>require(['QA']);</script>