                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card author-box card-primary">
                                <div class="card-body">
                                    <div class="author-box-left">
                                        @if (profile.fotoBool)
                                        {

                                            <img id="imgProfile" class="rounded-circle author-box-picture" src="@string.Format("data:image/jpeg;base64,{0}", Convert.ToBase64String(profile.foto))" height="100" alt=""> }
